{% set break = false%}
{% for item in pagination if not break %}
    <div class="col-md-4">
        <a href="{{ path('ed_frontend_blog_single_article', { 'slug': item.slug })}}"><img src="{% path item.excerptPhoto, 'big' %}" alt="YouAltText" title="YouAltText" style="width:100% !important;" />
            <br />
             {{item.title }}
        </a>
    </div>
    {% if loop.index == 4 %}
        {% set break = true %}
    {% endif %}
{% endfor %}
