{% set break = false%}
{% for item in pagination if not break %}
    <li>
        <a href="{{ path('ed_frontend_blog_single_article', { 'slug': item.slug })}}"><img src="{% path item.excerptPhoto, 'big' %}" alt="YouAltText" title="YouAltText" />
            <br />
             {{item.title }}
        </a>
    </li>
    {% if loop.index == 4 %}
        {% set break = true %}
    {% endif %}
{% endfor %}

