{% set break = false%}
{% for item in pagination if not break %}
    <div class="col-md-12" style="width:100% ; margin: auto; height: 150px; padding-left:0px;">
    <a class="black" href="{{ path('ed_frontend_blog_single_article', { 'slug': item.slug }) }}">
        <a class="black text--10 text--bold black margin--halfb lh32" href="{{ path('ed_frontend_blog_single_article', { 'slug': item.slug }) }}" "font-size:x-small;">
                        {{ item.title }}
        </a>
        <div style="width:100%">
            <img style="width:100% height: 100px;" class="" src="{% path item.excerptPhoto, 'big' %}">
        </div>
        
        
    </a>
    </div>
    {% if loop.index == 3 %}
        {% set break = true %}
    {% endif %}
{% endfor %}
